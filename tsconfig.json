{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "build",
    "engine",
    "examples",
    "extra",
    "libs",
    "samples",
    "tests"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "website/src/*"
      ]
    }
  }
}